{
  "name": "godwallet-backend",
  "version": "1.0.0",
  "description": "Backend API for Godwallet - Where domains find their dreams and possibilities multiply like pixie dust",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "deploy:contract": "hardhat run scripts/deploy.js --network sepolia",
    "verify:contract": "hardhat verify --network sepolia"
  },
  "keywords": [
    "domain",
    "lease",
    "marketplace",
    "web3",
    "nft",
    "blockchain"
  ],
  "author": "Godwallet Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-validator": "^7.0.1",
    "express-rate-limit": "^7.1.5",
    "morgan": "^1.10.0",
    "ethers": "^6.9.0",
    "axios": "^1.6.2",
    "qrcode": "^1.5.3",
    "stripe": "^14.9.0",
    "nodemailer": "^6.9.7",
    "bull": "^4.12.0",
    "redis": "^4.6.11",
    "winston": "^3.11.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "hardhat": "^2.19.2",
    "@openzeppelin/contracts": "^5.0.1",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/godwallet.git"
  },
